<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:background="@color/optimize_bg"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:orientation="vertical"
        android:background="@drawable/bg_border_uninstall"
        android:padding="@dimen/dp12"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:layout_margin="@dimen/dp12"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/native_ad_frame"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <com.android.launcher3.settings.custom.TextViewCustomFont
            android:textSize="18.0dip"
            android:textColor="@color/pro_main_text"
            android:id="@id/title"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="@string/are_you_sure_uninstall"
            android:includeFontPadding="false"
            app:customFont="2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <com.android.launcher3.views.SafeScrollView
            android:scrollbars="vertical"
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            android:layout_marginTop="4.0dip"
            android:layout_marginBottom="4.0dip"
            android:layout_marginVertical="4.0dip"
            app:layout_constraintBottom_toTopOf="@id/go_to_setting"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/title">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
                <com.android.launcher3.settings.custom.TextViewCustomFont
                    android:textSize="16.0dip"
                    android:textColor="@color/pro_main_text"
                    android:id="@id/description"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8.0dip"
                    android:text="@string/uninstall_warning_description"
                    android:includeFontPadding="false" />
                <com.android.launcher3.settings.custom.TextViewCustomFont
                    android:textSize="16.0dip"
                    android:textColor="@color/pro_main_text"
                    android:id="@id/warning_1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8.0dip"
                    android:text="@string/warning_point_1"
                    android:includeFontPadding="false"
                    android:layout_marginStart="6.0dip" />
                <com.android.launcher3.settings.custom.TextViewCustomFont
                    android:textSize="16.0dip"
                    android:textColor="@color/pro_main_text"
                    android:id="@id/warning_2"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8.0dip"
                    android:text="@string/warning_point_2"
                    android:includeFontPadding="false"
                    android:layout_marginStart="6.0dip" />
                <com.android.launcher3.settings.custom.TextViewCustomFont
                    android:textSize="16.0dip"
                    android:textColor="@color/pro_main_text"
                    android:id="@id/warning_3"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8.0dip"
                    android:text="@string/warning_point_3"
                    android:includeFontPadding="false"
                    android:layout_marginStart="6.0dip" />
            </LinearLayout>
        </com.android.launcher3.views.SafeScrollView>
        <TextView
            android:textSize="14.0dip"
            android:textColor="@color/white"
            android:gravity="center"
            android:id="@id/go_to_setting"
            android:background="@drawable/bg_ripple_button"
            android:padding="14.0dip"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20.0dip"
            android:layout_marginBottom="8.0dip"
            android:text="@string/button_go_to_setting"
            android:includeFontPadding="false"
            app:layout_constraintBottom_toTopOf="@id/continue_btn"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <TextView
            android:textSize="14.0dip"
            android:textColor="@color/blue_color"
            android:gravity="center"
            android:layout_gravity="center"
            android:id="@id/continue_btn"
            android:background="@drawable/bg_ripple_continue_button"
            android:padding="14.0dip"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8.0dip"
            android:text="@string/button_continue_uninstall"
            android:includeFontPadding="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <FrameLayout
        android:id="@id/native_ad_frame"
        android:paddingTop="@dimen/dp8"
        android:paddingBottom="@dimen/dp8"
        android:clipToPadding="false"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:paddingVertical="@dimen/dp8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.45" />
</androidx.constraintlayout.widget.ConstraintLayout>