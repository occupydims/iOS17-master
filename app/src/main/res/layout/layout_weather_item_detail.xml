<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <include
        android:id="@id/weather_cloud"
        android:background="@drawable/bg_weather_detail"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        app:layout_constraintDimensionRatio="w, 1:1"
        app:layout_constraintEnd_toStartOf="@id/weather_sunrise"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.45"
        layout="@layout/layout_weather_cloud" />
    <include
        android:id="@id/weather_sunrise"
        android:background="@drawable/bg_weather_detail"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        app:layout_constraintDimensionRatio="w, 1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/weather_cloud"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.45"
        layout="@layout/layout_weather_sunrise" />
    <include
        android:id="@id/weather_wind"
        android:background="@drawable/bg_weather_detail"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:layout_marginTop="16.0dip"
        android:layout_marginBottom="16.0dip"
        android:layout_marginVertical="16.0dip"
        app:layout_constraintDimensionRatio="w, 1:1"
        app:layout_constraintEnd_toStartOf="@id/weather_rain_volume"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/weather_cloud"
        app:layout_constraintWidth_percent="0.45"
        layout="@layout/layout_weather_wind" />
    <include
        android:id="@id/weather_rain_volume"
        android:background="@drawable/bg_weather_detail"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:layout_marginTop="16.0dip"
        android:layout_marginBottom="16.0dip"
        android:layout_marginVertical="16.0dip"
        app:layout_constraintDimensionRatio="w, 1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/weather_wind"
        app:layout_constraintTop_toBottomOf="@id/weather_sunrise"
        app:layout_constraintWidth_percent="0.45"
        layout="@layout/layout_weather_rain" />
    <include
        android:id="@id/weather_feels_like"
        android:background="@drawable/bg_weather_detail"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:layout_marginTop="16.0dip"
        android:layout_marginBottom="16.0dip"
        android:layout_marginVertical="16.0dip"
        app:layout_constraintDimensionRatio="w, 1:1"
        app:layout_constraintEnd_toStartOf="@id/weather_humidity"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/weather_wind"
        app:layout_constraintWidth_percent="0.45"
        layout="@layout/layout_weather_feels_like" />
    <include
        android:id="@id/weather_humidity"
        android:background="@drawable/bg_weather_detail"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:layout_marginTop="16.0dip"
        android:layout_marginBottom="16.0dip"
        android:layout_marginVertical="16.0dip"
        app:layout_constraintDimensionRatio="w, 1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/weather_feels_like"
        app:layout_constraintTop_toBottomOf="@id/weather_rain_volume"
        app:layout_constraintWidth_percent="0.45"
        layout="@layout/layout_weather_humidity" />
    <include
        android:id="@id/weather_visibility"
        android:background="@drawable/bg_weather_detail"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:layout_marginTop="16.0dip"
        android:layout_marginBottom="16.0dip"
        android:layout_marginVertical="16.0dip"
        app:layout_constraintDimensionRatio="w, 1:1"
        app:layout_constraintEnd_toStartOf="@id/weather_pressure"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/weather_feels_like"
        app:layout_constraintWidth_percent="0.45"
        layout="@layout/layout_weather_visibility" />
    <include
        android:id="@id/weather_pressure"
        android:background="@drawable/bg_weather_detail"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:layout_marginTop="16.0dip"
        android:layout_marginBottom="16.0dip"
        android:layout_marginVertical="16.0dip"
        app:layout_constraintDimensionRatio="w, 1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/weather_visibility"
        app:layout_constraintTop_toBottomOf="@id/weather_humidity"
        app:layout_constraintWidth_percent="0.45"
        layout="@layout/layout_weather_pressure" />
    <View
        android:layout_width="fill_parent"
        android:layout_height="48.0dip"
        app:layout_constraintTop_toBottomOf="@id/weather_pressure" />
</androidx.constraintlayout.widget.ConstraintLayout>