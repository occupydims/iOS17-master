<?xml version="1.0" encoding="utf-8"?>
<com.google.android.gms.ads.nativead.NativeAdView
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@id/ad_blur_background"
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <ImageView
            android:id="@id/ad_media_image"
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <ImageView
            android:id="@id/ad_media_pro"
            android:visibility="gone"
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <ImageView
            android:id="@id/ad_gradient_background"
            android:visibility="visible"
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <View
            android:background="@drawable/background_gradient_dim"
            android:visibility="visible"
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <androidx.constraintlayout.widget.ConstraintLayout
            android:padding="@dimen/margin_16dp"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">
            <androidx.cardview.widget.CardView
                android:id="@id/ad_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:elevation="4.0dip"
                app:cardCornerRadius="15.0dip"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <ImageView
                    android:id="@id/ad_app_icon"
                    android:layout_width="@dimen/native_ad_zero_page_image_size"
                    android:layout_height="@dimen/native_ad_zero_page_image_size"
                    android:adjustViewBounds="true" />
            </androidx.cardview.widget.CardView>
            <TextView
                android:textSize="14.5sp"
                android:textStyle="bold"
                android:textColor="@color/white"
                android:ellipsize="end"
                android:id="@id/ad_headline"
                android:layout_width="0.0dip"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16dp"
                android:maxLines="1"
                android:layout_marginEnd="8.0dip"
                app:layout_constraintBottom_toTopOf="@id/ad_body"
                app:layout_constraintEnd_toStartOf="@id/ad_call_to_action"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/ad_image" />
            <TextView
                android:textSize="13.0sp"
                android:textColor="@color/white"
                android:gravity="center"
                android:id="@id/ad_call_to_action"
                android:background="@drawable/bg_native_zero_page_action_button"
                android:paddingLeft="13.0dip"
                android:paddingTop="8.0dip"
                android:paddingRight="13.0dip"
                android:paddingBottom="8.0dip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAllCaps="false"
                android:paddingHorizontal="13.0dip"
                android:paddingVertical="8.0dip"
                app:layout_constraintBottom_toBottomOf="@id/ad_body"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/ad_headline" />
            <TextView
                android:id="@id/ads_mark"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp1"
                android:layout_marginEnd="4.0dip"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                style="@style/AdsMarkZeroPage" />
            <TextView
                android:textSize="12.0sp"
                android:textColor="@color/white"
                android:ellipsize="end"
                android:gravity="start"
                android:id="@id/ad_body"
                android:layout_width="0.0dip"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:layout_marginStart="4.0dip"
                android:layout_marginEnd="@dimen/dp8"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/ad_call_to_action"
                app:layout_constraintStart_toEndOf="@id/ads_mark" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</com.google.android.gms.ads.nativead.NativeAdView>