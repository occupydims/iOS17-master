<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@id/open_app_container"
    android:fitsSystemWindows="false"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <View
        android:id="@id/over_view"
        android:background="#65000000"
        android:focusable="true"
        android:fitsSystemWindows="false"
        android:clickable="true"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:background="@drawable/bg_app_ready"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@id/content_container"
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@id/ad_frame_open_app"
            app:layout_constraintEnd_toStartOf="@id/ad_frame_open_app_right"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <ImageView
                android:id="@id/close_btn"
                android:padding="12.0dip"
                android:layout_width="48.0dip"
                android:layout_height="48.0dip"
                android:layout_margin="16.0dip"
                android:src="@drawable/ic_clear"
                android:tint="#66696969"
                android:alpha="0.5"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
            <com.android.launcher3.settings.custom.TextViewCustomFont
                android:textSize="20.0dip"
                android:textColor="@color/pro_main_text"
                android:id="@id/title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp16"
                android:text="@string/the_app_ready"
                app:customFont="2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/close_btn" />
            <com.android.launcher3.settings.custom.TextViewCustomFont
                android:textSize="14.0dip"
                android:textColor="@color/pro_main_text"
                android:gravity="center"
                android:id="@id/content_app_ready"
                android:layout_width="0.0dip"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24.0dip"
                android:layout_marginTop="@dimen/dp4"
                android:layout_marginRight="24.0dip"
                android:text="@string/content_app_ready"
                android:layout_marginHorizontal="24.0dip"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/title" />
            <com.android.launcher3.settings.custom.TextViewCustomFont
                android:textSize="14.0dip"
                android:textColor="@color/blue_color"
                android:ellipsize="end"
                android:gravity="center"
                android:id="@id/open_btn"
                android:background="@drawable/bg_open_app_ready"
                android:paddingTop="@dimen/dp12"
                android:paddingBottom="@dimen/dp12"
                android:layout_width="0.0dip"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp16"
                android:text="@string/open_app"
                android:lines="1"
                android:includeFontPadding="false"
                android:layout_marginStart="@dimen/dp16"
                android:layout_marginEnd="@dimen/dp8"
                android:paddingVertical="@dimen/dp12"
                app:customFont="2"
                app:layout_constraintEnd_toStartOf="@id/go_back_btn"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/content_app_ready" />
            <com.android.launcher3.settings.custom.TextViewCustomFont
                android:textSize="14.0dip"
                android:textColor="@color/blue_color"
                android:ellipsize="end"
                android:gravity="center"
                android:id="@id/go_back_btn"
                android:background="@drawable/bg_open_app_ready"
                android:paddingTop="@dimen/dp12"
                android:paddingBottom="@dimen/dp12"
                android:layout_width="0.0dip"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp16"
                android:text="@string/go_back"
                android:lines="1"
                android:includeFontPadding="false"
                android:layout_marginStart="@dimen/dp8"
                android:layout_marginEnd="@dimen/dp16"
                android:paddingVertical="@dimen/dp12"
                app:customFont="2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/open_btn"
                app:layout_constraintTop_toBottomOf="@id/content_app_ready" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <View
            android:id="@id/divider_portrait_open_app"
            android:background="@color/onboarding_divider_background"
            android:visibility="gone"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/onboarding_divider_height"
            app:layout_constraintBottom_toTopOf="@id/ad_frame_open_app" />
        <FrameLayout
            android:id="@id/ad_frame_open_app"
            android:visibility="gone"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp16"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/content_container" />
        <View
            android:id="@id/divider_land_open_app"
            android:background="@color/onboarding_divider_background"
            android:visibility="gone"
            android:layout_width="@dimen/onboarding_divider_height"
            android:layout_height="fill_parent"
            app:layout_constraintEnd_toStartOf="@id/ad_frame_open_app_right" />
        <FrameLayout
            android:id="@id/ad_frame_open_app_right"
            android:visibility="gone"
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/content_container"
            app:layout_constraintWidth_percent="0.5" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>