<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="8.0dip"
    android:layout_marginRight="8.0dip"
    android:layout_marginHorizontal="8.0dip"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.android.launcher3.settings.custom.TextViewCustomFont
        android:textSize="14.0dip"
        android:textColor="@color/app_search_title_primary"
        android:ellipsize="end"
        android:id="@id/title"
        android:paddingTop="8.0dip"
        android:paddingBottom="8.0dip"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:text="@string/history"
        android:maxLines="1"
        android:lines="1"
        android:paddingEnd="16.0dip"
        android:paddingVertical="8.0dip"
        app:customFont="2"
        app:layout_constraintEnd_toStartOf="@id/delete"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <com.android.launcher3.settings.custom.TextViewCustomFont
        android:textSize="14.0dip"
        android:textColor="@color/app_search_title_secondary"
        android:id="@id/delete"
        android:paddingTop="8.0dip"
        android:paddingBottom="8.0dip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/delete"
        android:paddingVertical="8.0dip"
        app:customFont="2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <View
        android:id="@id/divider"
        android:background="@color/app_search_divider_color"
        android:layout_width="fill_parent"
        android:layout_height="0.5dip"
        app:layout_constraintTop_toBottomOf="@id/title" />
    <com.android.launcher3.appsearch.v2.ui.HistoryFlowLayout
        android:id="@id/apps_history"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-6.0dip"
        android:layout_marginTop="6.0dip"
        android:layout_marginRight="-6.0dip"
        android:overScrollMode="never"
        android:layout_marginHorizontal="-6.0dip"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider" />
</androidx.constraintlayout.widget.ConstraintLayout>