            android:paddingRight="6.0dip"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            style="@style/AdsMarkRight" />
        <TextView
            android:id="@id/ad_ic_inline"
            android:visibility="gone"
            android:layout_marginTop="@dimen/inline_ad_mark_margin_top"
            android:layout_marginStart="@dimen/all_apps_native_content_margin"
            android:layout_marginEnd="@dimen/all_apps_native_content_margin"
            app:layout_constraintStart_toEndOf="@id/ad_image"
            app:layout_constraintTop_toTopOf="parent"
            style="@style/AdsMaInline" />
        <TextView
            android:textSize="15.0dip"
            android:ellipsize="end"
            android:id="@id/ad_headline"
            android:paddingTop="@dimen/all_apps_native_padding"
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            android:maxLines="1"
            android:paddingStart="@dimen/all_apps_text_padding_start"
            android:paddingEnd="@dimen/ad_body_padding_end"
            app:layout_constraintBottom_toBottomOf="@id/ad_headline_temp"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:textSize="12.0sp"
            android:ellipsize="end"
            android:id="@id/ad_body"
            android:paddingTop="4.0dip"
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            android:maxLines="1"
            android:paddingStart="@dimen/all_apps_text_padding_start"
            android:paddingEnd="@dimen/ad_body_padding_end"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ad_headline" />
        <androidx.cardview.widget.CardView
            android:id="@id/ad_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/all_apps_native_padding"
            android:layout_marginStart="@dimen/all_apps_native_padding"
            app:cardCornerRadius="10.0dip"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <ImageView
                android:id="@id/ad_app_icon"
                android:layout_width="@dimen/native_ad_image_size"
                android:layout_height="@dimen/native_ad_image_size" />
        </androidx.cardview.widget.CardView>
        <TextView
            android:textSize="15.0dip"
            android:ellipsize="end"
            android:id="@id/ad_headline_temp"
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/all_apps_native_padding"
            android:maxLines="1"
            android:layout_marginStart="@dimen/all_apps_native_content_margin"
            android:layout_marginEnd="@dimen/all_apps_native_content_margin"
            app:layout_constraintEnd_toStartOf="@id/ad_call_to_action"
            app:layout_constraintStart_toEndOf="@id/ad_image"
            app:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:textSize="12.0dip"
            android:ellipsize="end"
            android:id="@id/ad_body_temp"
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_marginTop="4.0dip"
            android:maxLines="1"
            android:layout_marginStart="@dimen/all_apps_native_content_margin"
            android:layout_marginEnd="@dimen/all_apps_native_content_margin"
            app:layout_constraintEnd_toStartOf="@id/ad_call_to_action"
            app:layout_constraintStart_toEndOf="@id/ad_image"
            app:layout_constraintTop_toBottomOf="@id/ad_headline_temp" />
        <RatingBar
            android:id="@id/ad_stars"
            android:paddingBottom="@dimen/all_apps_native_padding"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4.0dip"
            android:layout_marginStart="@dimen/all_apps_native_content_margin"
            app:layout_constraintStart_toEndOf="@id/ad_image"
            app:layout_constraintTop_toBottomOf="@id/ad_body_temp"
            style="?android:ratingBarStyleSmall" />
        <Button
            android:textSize="13.0dip"
            android:textColor="@android:color/white"
            android:ellipsize="marquee"
            android:gravity="center"
            android:id="@id/ad_call_to_action"
