<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <corners
                android:radius="100.0dip" />
            <solid
                android:color="#ff007aff" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">
            <corners
                android:radius="100.0dip" />
            <gradient
                android:startColor="#bfffffff"
                android:endColor="#bfffffff"
                android:angle="-45.0"
                android:centerColor="#26ffffff" />
        </shape>
    </item>
    <item
        android:left="0.5dip"
        android:top="0.5dip"
        android:right="0.5dip"
        android:bottom="0.5dip">
        <shape
            android:shape="rectangle">
            <corners
                android:radius="100.0dip" />
            <solid
                android:color="#ff007aff" />
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_tick_white"
        android:left="12.0dip"
        android:top="12.0dip"
        android:right="12.0dip"
        android:bottom="12.0dip" />
</layer-list>